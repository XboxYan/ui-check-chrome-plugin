{
	"manifest_version": 2,
	"name": "ui-check",
	"version": "1.1.1",
	"description": "这是一个可以检测页面极限状态的插件，包括文本溢出，图片缺失，文本缺失等情况",
	"icons":
	{
		"16": "icon.png",
		"32": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": 
	{
		"default_icon": "icon.png",
		"default_title": "这是一个可以检测页面边界状态的插件",
		"default_popup": "popup.html"
	},
	"permissions": ["activeTab"],
	"homepage_url": "https://github.com/XboxYan/ui-check-chrome-plugin",
	"devtools_page": "popup.html"
}